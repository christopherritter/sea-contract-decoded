 be honored.","type":"section","prefixes":["4"],"prefix":"4","entire_prefix":"4","prefix_anchor":"4","level":1,"prior_prefix":"3","next_prefix":"5"},"7":{"id":"346","text":"Because of budget constraints, professional meeting reimbursements may be less than the actual anticipated and\/or incurred expenses. The administration shall make the employee aware of this limitation prior to the approval of the meeting.","type":"section","prefixes":["5"],"prefix":"5","entire_prefix":"5","prefix_anchor":"5","level":1,"prior_prefix":"4"}}}